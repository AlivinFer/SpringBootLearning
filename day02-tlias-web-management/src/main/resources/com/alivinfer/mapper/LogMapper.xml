<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.alivinfer.mapper.LogMapper">

    <select id="list" resultType="com.alivinfer.pojo.OperateLog">
        select
            o.*
        from
            operate_log o
        order by
            o.operate_time desc
    </select>
</mapper>